"VertexLitGeneric"
{
	"$basetexture" "models/bubblegun/bubblegun_film"
//	"$basemapalphaphongmask"	"1"
	"$model" 			"1"

//	"$nocull" 1
	"$selfillum" 			"1"
	"$translucent" 			"1"
	"$additive" 			"1"

	"$nodecal"			"1"

	"$envmap"			"env_cubemap"
	"$envmaptint"			"[.6 .7 1.5]"

        "$angle" 			".0"
        "$translate" 			"[0.0 0.0]"
        "$center" 			"[.5 .5]"

	"Proxies"
	{


            "LinearRamp"
            {
                "rate" 180
                "initialValue" 0.0
                "resultVar" "$angle"
            }

            "TextureTransform"
            {
                "translateVar" "$translate"
                "rotateVar" "$angle"
                "centerVar" "$center"
                "resultVar" "$basetexturetransform"
            }


	}
}
