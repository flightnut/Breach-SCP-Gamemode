"Refract"
{
	"$model" "1"
	"$translucent" 1
	"$refractamount" ".25"
	"$refracttint" "[1 1 1]"

	$envmap env_cubemap
	$envmaptint "[1 1 1]"

	"$dudvmap" "Morganicism/MetroRedux/gasmask/ui_maska_distort_6"
	"$normalmap" "Morganicism/MetroRedux/gasmask/ui_maska_distort_6"

	"$bumpframe" "0"

	"$REFRACTTINTTEXTURE" "Morganicism/MetroRedux/gasmask/ui_maska_d_6"

	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$refracttinttexture"
			"animatedtextureframenumvar" "$bumpframe"
			"animatedtextureframerate" 30.00
		}

	}
}
